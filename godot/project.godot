; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Color Test"
config/description="A prototype of a game mechanism idea from color test"
run/main_scene="res://color-test.tscn"
config/icon="res://icon.png"

[audio]

default_bus_layout=""

[display]

window/size/width=1125
window/size/height=2436
window/size/always_on_top=true
window/size/test_width=375
window/size/test_height=812
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/physics_fps=120
common/enable_pause_aware_picking=true
2d/physics_engine="GodotPhysics"
2d/thread_model=2

[rendering]

environment/default_clear_color=Color( 1, 1, 1, 1 )
environment/default_environment="res://default_env.tres"
