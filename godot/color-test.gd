#
# Godot-color-test   by Jianhao Zheng
# 
#
# Made as a prototype for a mobile game mechanism, might need double click when interacting on PCs
# 
# Chart data extracted from public domain asset
# 	https://commons.wikimedia.org/wiki/File:Ishihara_9.svg
#

extends Node2D

var ball = load("res://ball.tscn")  # instance of ball

const resize_scale = 0.027  # tested value for scaling
const default_color = Color(0.5, 0.5, 0.5, 1)  # default gray color

var color_image = load("res://test-image.jpg")  # load the reference image
var color_data  # variable storing its pixel data

# Variables for counting
var instanced_count = 0
var colored_count = 0

const loop_step = 20	# increment step of balls added each frame
const set_size 	= 708	# size of the set or total 

# DEFINE INTERACTION MODES
# 	Coloring: reveal the color of the balls at touch
# 	Breaking: enable the physics simulation at touch
# 	None    : do nothing
export(int, "Coloring", "Breaking", "None") var touch_mode

var colored_needed	= 650	# number of colored balls needed to trigger break mode
var broken_left		= 100	# number of balls left on screen to stop interations

# Nodes
onready var ishihara = $Ishihara
onready var coloring = $coloring
onready var breaking = $breaking

func _ready():
	
	# get pixel data
	color_data = color_image.get_data()
	color_data.lock()
	
	pass

func _physics_process(delta):
	
	# initialize the chart in a for loop
	# 	only add several objects each frame to avoid freezing
	if (instanced_count < set_size):
		for i in loop_step:
			draw_ball(i)
		instanced_count += loop_step
	
	pass

func draw_ball(i):
	
	# number of this instance
	var j = instanced_count + i
	
	# stop when reaching the end of the data array
	if (j >= ishihara_set.size()):
		return
	
	# create an instance
	var new = ball.instance()
	
	# remapping the position data
	# 360 * 2.8 = 1008
	new.pos_x = ishihara_set[j][0] * 2.8 - 504
	new.pos_y = ishihara_set[j][1] * 2.8 - 504
	
	# assign size and color
	new.size = ishihara_set[j][2] * resize_scale
	new.tint =  default_color
	
	# add this instance to the node tree
	ishihara.add_child(new)
	
	pass

func _input(event: InputEvent) -> void:
	
	# touch event triggered
	if event is InputEventScreenTouch and event.is_pressed():
		
		# set corresponding collision object to touch position
		if touch_mode == 0:
			coloring.position = to_local(event.position)
		elif touch_mode == 1:
			breaking.position = to_local(event.position)
	
	pass

# Coloring the balls according to the color data from the reference image
func _on_coloring_body_entered(body):
	
	# if the collided object is a ball
	if 'ball' in body.name:
		
		# remapping screen position to the actual pixel on the image
		# reversed from 360 * 2.8 = 1008 
		var xx = round( (body.pos_x + 504) / 2.8 )
		var yy = round( (body.pos_y + 504) / 2.8 )
		
		# calling the coloring function of ball object
		# 	if the ball has not been colored, count 1
		# 	if the ball has been colored, ignore it
		if body.coloring( color_data.get_pixel(xx, yy) ):
			colored_count += 1
		
		# switching to breaking mode when reached the number
		if colored_count > colored_needed:
			touch_mode = 1

# Activate the physics of touched balls
func _on_breaking_body_entered(body):
	
	# if the collided object is a ball
	if 'ball' in body.name:
		
		body.activate()
		
		# switching to none mode when reached the number
		if ishihara.get_children().size() < broken_left:
			touch_mode = 2

